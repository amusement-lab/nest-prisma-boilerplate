{
  "name": "nest-prisma-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "concurrently --kill-others \"pnpm run test:env && pnpm run test:run\" \"wait-on tcp:3030 && pnpm run test:run\"",
    "test:env": "dotenv -e .env.test -- pnpm exec prisma migrate reset --force && dotenv -e .env.test -- pnpm exec prisma db push && dotenv -e .env.test -- pnpm seed && dotenv -e .env.test -- nest start",
    "test:run": "dotenv -e .env.test -- vitest run",
    "ncu": "ncu",
    "ncu:u": "ncu -u",
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.2.7",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.2.7",
    "@nestjs/platform-express": "^10.2.7",
    "@open-draft/until": "^2.1.0",
    "@prisma/client": "^5.4.2",
    "date-fns": "^2.30.0",
    "dotenv": "^16.4.5",
    "jsonwebtoken": "^9.0.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.7",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.93",
    "@types/express": "^4.17.19",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.8.6",
    "@types/superagent": "^8.1.7",
    "@types/supertest": "^2.0.14",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@vitest/coverage-v8": "^1.6.0",
    "concurrently": "^8.2.2",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "npm-check-updates": "^16.14.6",
    "prettier": "^3.0.3",
    "prisma": "^5.4.2",
    "source-map-support": "^0.5.21",
    "superagent": "^9.0.2",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "unplugin-swc": "^1.4.5",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0",
    "wait-on": "^7.2.0"
  }
}
